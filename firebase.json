{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "sitemap.xml",
        "destination": "/sitemap.xml"
      },
      {
        "source": "**",
        "function": "app"
      }
    ],
    "headers": [
      {
        "source": "ngsw-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  }
}
